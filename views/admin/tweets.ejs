<h3 class="mb-3"><i class="fab fa-twitter me-2"></i>Tweet Logs</h3>
<table class="table">
  <thead><tr><th>Time</th><th>User</th><th>Type</th><th>Content</th><th>Status</th></tr></thead>
  <tbody>
    <% rows.forEach(r => { %>
      <tr>
        <td><%= r.createdAt.toLocaleString() %></td>
        <td><%= r.User?.email %></td>
        <td><%= r.type %></td>
        <td><%= (r.content||'').slice(0,80) %></td>
        <td><%= r.error ? ('❌ ' + r.error.slice(0,60)) : '✅ '+(r.twitterId||'-') %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
