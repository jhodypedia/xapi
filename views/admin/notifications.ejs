<h3 class="mb-3"><i class="fa fa-inbox me-2"></i>All Notifications</h3>
<div class="notice-list">
  <% rows.forEach(n => { %>
    <div class="notice">
      <div class="ic"><i class="fa fa-bell"></i></div>
      <div class="body">
        <div class="title"><%= n.title %> <span class="text-muted">â€” <%= n.User?.email %></span></div>
        <div class="time"><%= n.createdAt.toLocaleString() %></div>
        <div><%= n.message %></div>
      </div>
    </div>
  <% }) %>
</div>
