<h3 class="mb-3"><i class="fa fa-right-to-bracket me-2"></i>Login Logs</h3>
<table class="table">
  <thead><tr><th>Time</th><th>User</th><th>IP</th><th>UA</th><th>Location</th></tr></thead>
  <tbody>
    <% rows.forEach(r => { %>
      <tr>
        <td><%= r.createdAt.toLocaleString() %></td>
        <td><%= r.User?.email %></td>
        <td><%= r.ip %></td>
        <td><%= r.userAgent?.slice(0,60) %>...</td>
        <td><%= r.location || '-' %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
